{
  "name": "smee-client",
  "version": "0.0.0-development",
  "description": "Client to proxy webhooks to localhost",
  "main": "index.js",
  "bin": {
    "smee": "./bin/smee.js"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "bin"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
