{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "last 2 versions",
          "ios_saf >= 8",
          "ie >= 10",
          "chrome >= 49",
          "firefox >= 49",
          "> 1%"
        ]
      },
      "debug": false,
      "loose": true,
      "useBuiltIns": true
    }],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    [
      "transform-object-rest-spread",
      { "useBuiltIns": true }
    ]
  ]
}