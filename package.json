{
  "name": "smee-client",
  "version": "0.0.0-development",
  "description": "Client to proxy webhooks to localhost",
  "main": "index.js",
  "type": "module",
  "bin": {
    "smee": "bin/smee.js"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "bin"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "prettier --check \"index.ts\" \"test/**/*.ts\" bin/smee.js package.json tsconfig.json --end-of-line auto",
    "lint:fix": "prettier --write \"index.ts\" \"test/**/*.ts\" bin/smee.js package.json tsconfig.json --end-of-line auto",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:dev": "vitest --ui --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/probot/smee-client.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^12.0.0",
    "eventsource": "^3.0.5",
    "undici": "^7.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@octokit/tsconfig": "^4.0.0",
    "@types/node": "^20.0.0",
    "@types/validator": "^13.11.6",
    "@vitest/coverage-v8": "^3.0.0",
    "fastify": "^5.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
